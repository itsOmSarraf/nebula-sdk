{
  "name": "0g-agent-contracts",
  "version": "1.0.0",
  "description": "Smart contracts for 0G AI Agent SDK - On-chain agent identity and token deployment",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:sdk": "hardhat run scripts/deploy-for-sdk.js",
    "deploy:local": "hardhat run scripts/deploy-for-sdk.js --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy-for-sdk.js --network zerogTestnet",
    "verify": "hardhat verify",
    "coverage": "hardhat coverage",
    "size": "hardhat size-contracts",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write 'contracts/**/*.sol'",
    "clean": "hardhat clean",
    "node": "hardhat node"
  },
  "keywords": [
    "solidity",
    "smart-contracts",
    "0g",
    "ai",
    "agent",
    "erc20",
    "erc721",
    "nft",
    "factory",
    "openzeppelin"
  ],
  "author": "Om Sarraf",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.6",
    "@nomicfoundation/hardhat-ignition": "^0.15.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.8",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "chai": "^4.3.10",
    "ethers": "^6.13.1",
    "hardhat": "^2.22.5",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.10",
    "prettier": "^3.3.2",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^5.0.1",
    "solidity-coverage": "^0.8.12",
    "typechain": "^8.3.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0glabs/0g-agentic-sdk"
  },
  "bugs": {
    "url": "https://github.com/0glabs/0g-agentic-sdk/issues"
  },
  "homepage": "https://github.com/0glabs/0g-agentic-sdk#readme"
}
